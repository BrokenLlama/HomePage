{
  "name": "homepage",
  "version": "1.0.0",
  "description": "Portal for a browser with my most used stuff",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix frontend",
    "start": "nodemon ./server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd frontend && npm install && npm run build"
  },
  "author": "Eduardo Araujo",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http": "0.0.1-security",
    "moment": "^2.26.0",
    "puppeteer": "^3.1.0",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "sweetalert2": "^9.14.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
